<scenefile>
	<globaldata>
		<diffusecoeff v="0.5"/>
		<specularcoeff v="0.5"/>
		<ambientcoeff v="0.5"/>
	</globaldata>

	<cameradata>
		<pos x="-4" y="3" z="3"/>
		<up x="0" y="1" z="0"/>
		<focus x="0" y="0" z="0"/>
		<heightangle v="30"/>
	</cameradata>

	<lightdata>
		<id v="0"/>
		<color r="0.5" g="0.5" b="0.5"/>
		<position x="10" y="10" z="10"/>
	</lightdata>

	<lightdata>
		<id v="1"/>
		<color r="0.3" g="0.3" b="0.3"/>
		<position x="0" y="10" z="0"/>
	</lightdata>

	<object type="tree" name="root">
		<transblock>
			<translate x="0" y="-0.6" z="0"/>
			<scale x="6" y="0.2" z="6"/>
			<object type="primitive" name="cube" >
				<texture file="image/cheese.png" u="1" v="1"/>
				<diffuse r="1" g="1" b="0.5"/>
				<blend v="0.3"/>
			</object>
		</transblock>
		<transblock>
			<translate x="-0.8" y="0" z="-0.8"/>
			<rotate x="0" y="1" z="1" angle="45"/>
			<object type="primitive" name="sphere" >
				<texture file="image/cheese.png" u="1" v="1"/>
				<reflective r="0.3" g="0.3" b="0.3"/>
				<diffuse r="0" g="1" b="0"/>
				<blend v="0.5"/>
				<specular r="0" g="0" b="0"/>
			</object>
		</transblock>
		<transblock>
			<translate x="0.8" y="0" z="-0.8"/>
			<object type="primitive" name="cube" >
				<reflective r="0.5" g="0.5" b="0.5"/>
				<texture file="image/cheese.png" u="1" v="1"/>
				<blend v="0.8"/>
			</object>
		</transblock>
		<transblock>
			<translate x="0.8" y="0" z="0.8"/>
			<object type="primitive" name="cylinder" >
				<reflective r="0.9" g="0.9" b="0.9"/>
				<diffuse r="1.0" g="1.0" b="1.0"/>
				<texture file="image/cheese.png" u="1" v="1"/>
				<blend v="1.0"/>
			</object>
		</transblock>
		<transblock>
			<translate x="-0.8" y="0" z="0.8"/>
			<object type="primitive" name="cone" >
				<texture file="image/cheese.png" u="1" v="1"/>
				<blend v="1.0"/>
			</object>
		</transblock>
	</object>

</scenefile>

